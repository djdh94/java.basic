import java.util.Scanner;

public class test {

	public static void main(String[] args) {
		
		int[] math = {96,23,52,82,72,31,58};
		
		int total=0;
		int count=0;
		
		for(int a:math)
		{
		
			if(a<60)
			{
				continue;
			}else
			{
			count+=1;
			total+=a;
			}
		}System.out.println("수학평균:"+total);
	
	}	
}