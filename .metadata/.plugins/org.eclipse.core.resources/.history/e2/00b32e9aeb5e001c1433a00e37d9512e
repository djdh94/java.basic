package polymorphism;

public class Student2 extends Person{
	
	private int num2;
	
public Student2(String name, int age, int num2) {
		
		super(name,age);
		this.num2=num2;
	}

public void showPerosn() {
	super.showPerson();
	System.out.println("학생: "+num2);
}

public void showStudent2() {
	System.out.println("저는 학생 입니다");
}

}
