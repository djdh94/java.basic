package test;

public class Warrior extends Commoner{

	private int attack;
	private int doubleAttack;
	
	
	public void attack() {
		System.out.println("공격");
		setHp(getHp()-2);
	}
	
	public void doubleAttack() {
		System.out.println("더블공격");
		setMp(getMp()-3);
	}
	
	public void getInfo() {
		System.out.println("현재 체력:"+getHp()+"현재 경험치:"+getExp()+",현재 마나:"+getMp());
	}
}
