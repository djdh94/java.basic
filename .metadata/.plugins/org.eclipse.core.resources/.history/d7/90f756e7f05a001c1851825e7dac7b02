package structure;

public class D {

	public static void getDoginfo(Dog d) {
		System.out.println("개이름:"+d.name);
	}
	public static void main(String[] args) {
		
		Dog d = new Dog();
		d.name="김대현";
		d.age=28;
		
		
		Dog d2= new Dog();
		d2.name="김승곤";
		d2.age=28;

		getDoginfo(d);
		getDoginfo(d2);
	}

}
