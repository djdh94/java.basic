import java.util.Scanner;

public class test {

	public static void main(String[] args) {
		
	Scanner scan = new Scanner(System.in);
	System.out.println("정수입력");
	int num = scan.nextInt();
	
	int i=0;
	while(i<num) {
		
		int j=0;
		while(j<num)
		{
		System.out.print("*");
		j++;
		}i++;
	}
	
	
	}
}
		
