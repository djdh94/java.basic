package structure;

public class test2 {

	public int height;
	public int jumpHeight;
	

	public test2(int h,int j) {
		
		height=h;
		jumpHeight=j;
		System.out.println("생성된 선수키: "+h);
		System.out.println("점프력: "+j);
	}
	
	public void dunkShoot() {
		if(height+jumpHeight>300) {
			
		System.out.println("덩크슛 성공");
			
		}else
			System.out.println("덩크슛 실패");
	}
	
	
}
