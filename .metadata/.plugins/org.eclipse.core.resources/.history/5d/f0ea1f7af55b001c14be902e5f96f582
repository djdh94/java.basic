package intheritance;

public class Basket {

	public int height;
	public int jumpHeight;
	
	public Basket(int h, int j) {
		
		height=h;
		jumpHeight=j;
		
		System.out.println("생성된 선수 키: "+height+", 점프력: "+jumpHeight);
		
	}
	
	public void dunkShoot() {
		if(height+jumpHeight>300)
		{
			System.out.println("덩크슛 성공");
		}else
			System.out.println("덩크슛 실패");
	}
}
