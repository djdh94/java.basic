package test;

public class Commoner {

	 protected int lv;
	 protected int hp;
	 protected int mp;
	 protected String name;
	 
	 public Commoner(String name) {
		 this.lv=1;
		 this.hp=20;
		 this.mp=10;
		 this.name=name;
	 }
	 
	 public void getInfo() {
		 
		 System.out.println("캐릭터를 생성합니다.");
		 System.out.println("아이디:"+name+",체력:"+hp+",마나:"+mp+",레벨:"+lv);
	 }
	 
}
