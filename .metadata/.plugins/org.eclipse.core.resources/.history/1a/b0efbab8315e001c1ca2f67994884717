		Orc o1 = new Orc();
		
		a1.huntOrc(o1);
		a1.huntOrc(o1);
		a1.huntOrc(o1);
		
		Troll t1 = new Troll();
		a1.huntTroll(t1);
		a1.huntTroll(t1);
		a1.huntTroll(t1);
			
		
	}
}
