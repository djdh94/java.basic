package constructor;

public class BasketballPlayer {

	public String player;
	public int height;
	public int jumpHeight;
	
	public BasketballPlayer(int height,int jumpHight) {
		
		player = "김대현";
		height = 180;
		jumpHeight=50;
		System.out.println("선수명:"+player+",키:"+height+",점프력:"+jumpHight);

	}
	
	public void getInfo() {
		
		System.out.println();
	
	
		
	}
		
	
}
