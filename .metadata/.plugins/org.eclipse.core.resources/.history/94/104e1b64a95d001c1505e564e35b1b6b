package test;

public class Commoner {

	 private int lv;
	 private int hp;
	 private int mp;
	 private int exp;
	 
	 public Commoner() {
		 this.lv=1;
		 this.hp=20;
		 this.mp=10;
		 this.exp=0;
	 }

	public int getLv() {
		return lv;
	}

	public void setLv(int lv) {
		this.lv = lv;
	}

	public int getHp() {
		return hp;
	}

	public void setHp(int hp) {
		this.hp = hp;
	}

	public int getMp() {
		return mp;
	}

	public void setMp(int mp) {
		this.mp = mp;
	}

	public int getExp() {
		return exp;
	}

	public void setExp(int exp) {
		this.exp = exp;
	}
	 
	
	 
}
