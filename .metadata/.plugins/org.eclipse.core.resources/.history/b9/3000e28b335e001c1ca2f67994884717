package game.interaction;

public class Main03 {

	public static void main(String[] args) {
		
		Archer a1 = new Archer();
		
		Monster m1 = new Monster();
		a1.huntMonster(m1);
		a1.huntMonster(m1);
		a1.huntMonster(m1);
			
		Warrior w1 = new Warrior();
	
		Orc o1 = new Orc();
		w1.huntOrc(o1);
		w1.huntOrc(o1);
		w1.huntOrc(o1);
		w1.huntOrc(o1);
		w1.huntOrc(o1);
		w1.huntOrc(o1);
		w1.huntOrc(o1);
	}
}
