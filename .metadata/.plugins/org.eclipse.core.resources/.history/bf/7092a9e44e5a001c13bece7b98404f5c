import java.util.Scanner;

public class test {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		System.out.println("정수입력");
		
		int num = scan.nextInt();
		
		for(int i=0; i<3; i++) 
		{
			
			for(int j=0; j<3; j++ ) 
			{
			System.out.print("*");	
			}System.out.println();
		}
	}
}
		
