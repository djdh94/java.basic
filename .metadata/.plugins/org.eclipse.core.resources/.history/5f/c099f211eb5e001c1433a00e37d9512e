package polymorphism;

public class Teacher extends Person{
	
	private int num;
	
	public Teacher(String name, int age, int num) {
		
		super(name,age);
		this.num=num;
	}	
	
	public void showPerosn() {
		super.showPerson();
		System.out.println("선생님 숫자: "+num);
	}
	
	public void showTeacher() {
		System.out.println("저는 선생님 입니다");
	}

}
